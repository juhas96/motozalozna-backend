          "example" : "VT657CG"
        },
        "car_km" : {
          "type" : "number",
          "example" : 120000.0
        },
        "car_damaged_varnish" : {
          "type" : "boolean",
          "example" : false
        },
        "car_damaged_bodywork" : {
          "type" : "boolean",
          "example" : false
        },
        "car_damaged_axle" : {
