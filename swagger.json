          "example" : "2020-06-26T10:39:55.000Z"
        },
        "loan_price" : {
          "type" : "number",
          "example" : 12000.0
        },
        "loan_length" : {
          "type" : "string",
          "example" : "2 tyzdne"
        },
        "interest_paid" : {
          "type" : "boolean",
          "example" : false
        },
        "car_bodywork_type" : {
          "type" : "string",
          "example" : "Hatchbag / Sedan"
        },
        "car_fuel_type" : {
