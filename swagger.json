{
  "swagger" : "2.0",
  "info" : {
    "description" : "Motozalozna backend API",
    "version" : "0.0.1",
    "title" : "Motozalozna API",
    "contact" : {
      "email" : "juhas.jugi@gmail.com"
    },
    "license" : {
      "name" : "Apache 2.0",
      "url" : "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "host" : "motozalozna-backend.herokuapp.com",
  "basePath" : "/api",
  "tags" : [ {
    "name" : "loans",
    "description" : "All endpoints for loans"
  }, {
    "name" : "users",
    "description" : "All endpoints for user"
  }, {
    "name" : "form",
    "description" : "All endpoints for FORM, check price, stolen etc."
  }, {
    "name" : "auth",
    "description" : "All endpoints for authentication"
  } ],
  "schemes" : [ "http" ],
  "paths" : {
    "/auth/login" : {
      "post" : {
        "tags" : [ "auth" ],
        "summary" : "Login",
        "operationId" : "login",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "name" : "email",
          "in" : "body",
          "description" : "User email",
          "required" : true,
          "type" : "string"
        }, {
          "name" : "password",
          "in" : "body",
          "description" : "User pass",
          "required" : true,
          "type" : "string"
        } ],
        "responses" : {
          "200" : {
            "description" : "login was successful"
          },
          "500" : {
            "description" : "server error"
          }
        }
      }
    },
    "/auth/logout" : {
      "delete" : {
        "tags" : [ "auth" ],
        "summary" : "Logout",
        "operationId" : "logout",
        "produces" : [ "application/json" ],
        "responses" : {
          "204" : {
            "description" : "logout was successful"
          },
          "500" : {
            "description" : "server error"
          }
        }
      }
    },
    "/loans" : {
      "get" : {
        "tags" : [ "loans" ],
        "summary" : "Returs all Loans with user objects",
        "operationId" : "findAll",
        "produces" : [ "application/json" ],
        "parameters" : [ ],
        "responses" : {
          "200" : {
            "description" : "all results",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/Loan"
              }
            }
          },
          "500" : {
            "description" : "server error"
          }
        }
      }
    },
    "/loans/:id" : {
      "get" : {
        "tags" : [ "loans" ],
        "summary" : "Returs single loan with user object",
        "operationId" : "findOne",
        "produces" : [ "application/json" ],
        "parameters" : [{
          "name" : "id",
          "in" : "path",
          "description" : "The loan ID",
          "required" : true,
          "type" : "integer"
        }],
        "responses" : {
          "200" : {
            "description" : "single result",
            "schema" : {
              "type" : "object",
              "items" : {
                "$ref" : "#/definitions/Loan"
              }
            }
          },
          "500" : {
            "description" : "server error"
          }
        }
      }
    },
    "/loans/by_user" : {
      "get" : {
        "tags" : [ "loans" ],
        "summary" : "Returs all Loans for specific user",
        "operationId" : "byUser",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "name" : "user_id",
          "in" : "header",
          "description" : "The user ID",
          "required" : true,
          "type" : "integer"
        } ],
        "responses" : {
          "200" : {
            "description" : "all loans for single user",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/Loan"
              }
            }
          },
          "500" : {
            "description" : "server error"
          }
        }
      }
    },
    "/loans/pay" : {
      "post" : {
        "tags" : [ "loans" ],
        "summary" : "Handle pay callback from frontEnd",
        "operationId" : "pay",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "name" : "stripeTokenId",
          "in" : "body",
          "description" : "TOKEN ID",
          "required" : true,
          "type" : "string"
        }, {
          "name": "price",
          "in": "body",
          "description": "Price to charge",
          "required": true,
          "type": "number"
        } ],
        "responses" : {
          "200" : {
            "description" : "successfully payed"
          },
          "500" : {
            "description" : "server error"
          }
        }
      }
    },
    "/form/upload" : {
      "post" : {
        "tags" : [ "form" ],
        "summary" : "Returs all Loans for specific user",
        "operationId" : "upload",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "in" : "body",
          "name" : "form-data",
          "description" : "All data from form.",
          "required" : false,
          "schema" : {
            "type" : "object"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "success"
          },
          "500" : {
            "description" : "server error"
          }
        }
      }
    },
    "/form/check_price" : {
      "post" : {
        "tags" : [ "form" ],
        "summary" : "Returs price of car",
        "operationId" : "checkPrice",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "in" : "body",
          "name" : "form-data",
          "description" : "All data from car form.",
          "required" : false,
          "schema" : {
            "$ref" : "#/definitions/Price_Check"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "return price"
          },
          "500" : {
            "description" : "server error"
          }
        }
      }
    },
    "/form/check_stolen" : {
      "post" : {
        "tags" : [ "form" ],
        "summary" : "Returs if car is stolen",
        "operationId" : "checkStolen",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "in" : "body",
          "name" : "ecv",
          "description" : "ECV of car.",
          "required" : false,
          "schema" : {
            "$ref" : "#/definitions/Stolen_Check"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "return number value"
          },
          "500" : {
            "description" : "server error"
          }
        }
      }
    }
  },
  "definitions" : {
