        },
        "car_damaged_tires" : {
          "type" : "boolean",
          "example" : false
        },
        "car_damaged_window" : {
          "type" : "boolean",
          "example" : false
        },
        "car_price" : {
          "type" : "number",
          "example" : 12000.0
        },
        "interest" : {
          "type" : "number",
          "example" : 400.0
        },
        "createdAt" : {
          "type" : "string",
          "example" : "2020-06-26T10:39:55.000Z"
        },
        "updatedAt" : {
          "type" : "string",
          "example" : "2020-06-26T10:39:55.000Z"
        },
        "userId" : {
          "type" : "number",
          "example" : 1.0
        },
        "user" : {
          "$ref" : "#/definitions/User"
        }
      }
    },
    "User" : {
      "properties" : {
        "id" : {
          "type" : "number",
          "example" : 1.0
        },
        "first_name" : {
          "type" : "string",
          "example" : "Jakub"
        },
        "last_name" : {
          "type" : "string",
          "example" : "Juh√°s"
        },
        "email" : {
